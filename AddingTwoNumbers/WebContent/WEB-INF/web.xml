<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>AddingTwoNumbers</display-name>
  
  <servlet>
	  <servlet-name>Addition</servlet-name>
	  <servlet-class>com.servlet.AddServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
	  <servlet-name>Addition</servlet-name>
	  <url-pattern>/add</url-pattern>
  </servlet-mapping>
  
  <servlet>
	  <servlet-name>Printing</servlet-name>
	  <servlet-class>com.servlet.PrintServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
	  <servlet-name>Printing</servlet-name>
	  <url-pattern>/print</url-pattern>
  </servlet-mapping>
  
</web-app>